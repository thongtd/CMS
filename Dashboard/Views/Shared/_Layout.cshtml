<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Dashboard</title>

    <environment names="Development">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
        <title>@ViewBag.Title</title>
        <link rel="icon" type="image/ico" href="~/Dashboard/favicon.ico" />
        <link href="~/Dashboard/css/stylesheets.css" rel="stylesheet" type="text/css" />
        <link rel='stylesheet' type='text/css' href='~/Dashboard/css/fullcalendar.print.css' media='print' />
        <link href="~/Dashboard/css/custom.css" rel="stylesheet" />
        <script src="~/Dashboard/js/jquery-1.7.2.min.js"></script>
        <script src="~/Dashboard/js/jquery-ui.min.js"></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/jquery/jquery.mousewheel.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/cookie/jquery.cookies.2.2.0.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/bootstrap.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/charts/jquery.flot.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/plugins/charts/jquery.flot.stack.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/plugins/charts/jquery.flot.pie.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/plugins/charts/jquery.flot.resize.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/sparklines/jquery.sparkline.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/fullcalendar/fullcalendar.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/select2/select2.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/uniform/uniform.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/maskedinput/jquery.maskedinput-1.3.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/validation/languages/jquery.validationEngine-en.js' charset='utf-8'></script>
        <script type='text/javascript' src='~/Dashboard/js/plugins/validation/jquery.validationEngine.js' charset='utf-8'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/plugins/animatedprogressbar/animated_progressbar.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/qtip/jquery.qtip-1.0.0-rc3.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/cleditor/jquery.cleditor.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/dataTables/jquery.dataTables.min.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/plugins/fancybox/jquery.fancybox.pack.js'></script>

        <script type='text/javascript' src='~/Dashboard/js/cookies.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/actions.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/charts.js'></script>
        <script type='text/javascript' src='~/Dashboard/js/plugins.js'></script>
        <script src="~/Dashboard/plugins/ckeditor/ckeditor.js"></script>
        <script src="~/Dashboard/plugins/ckfinder/ckfinder.js" type="text/javascript"></script>
        <script src="~/Dashboard/js/project.js"></script>
        <script src="~/Dashboard/lib/jquery.validate.min.js" type="text/javascript"></script>
        <script src="~/Dashboard/lib/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
        <style type="text/css">
            .mg-left-none {
                margin-left: 0px !important;
            }
        </style>
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
    </environment>
    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
    <div class="header">
        <a class="logo" href="http://webvietgroup.com/" target="_blank">
            <img src="~/Dashboard/img/logo-wvg-mini.png" alt="thiet-ke-web" title="WebVietGroup - Nơi khởi đầu cho một thương hiệu" />
        </a>
        <ul class="header_menu">
            <li class="list_icon"><a href="#">&nbsp;</a></li>
        </ul>
    </div>
    <div class="menu">
        <div class="breadLine">
            <div class="arrow"></div>
            <div class="adminControl active">
                Hi, FullName
            </div>
        </div>

        <div class="admin">
            <div class="image">
                <img src="~/Dashboard/img/users/aqvatarius.jpg" class="img-polaroid" />
            </div>
            <ul class="control">
                <li><span class="icon-comment"></span><a href="#">Tin nhắn</a> <a href="#" class="caption red">0</a></li>
                <li><span class="icon-cog"></span><a href="@Url.Action("Setting", "Auth", new { area = "Auth" })">Cài đặt</a></li>
                <li><span class="icon-share-alt"></span><a href="@Url.Action("Logout", "Auth", new { area = "Auth" })">Thoát</a></li>
            </ul>
            <div class="info">
                <span>Welcom back!</span>
            </div>
        </div>

        <ul class="navigation">
            <li>
                <a href="/">
                    <span class="isw-grid"></span><span class="text">Home</span>
                </a>
            </li>
            <li class="openable @ViewBag.Menu">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Menu</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "Menu", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách Menu</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Create", "Menu", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Thêm mới Menu</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.News">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Bài viết </span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "GroupBlog", new {area = "Admin"})">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách nhóm bài viết</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Blog", new {area = "Admin"})">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách bài viết</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.Club">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Đội bóng</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "Club", new {area = "Admin"})">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách đội bóng</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.MatchInfo">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Trực tiếp</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "Tournament", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách giải đấu</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "MatchInfo", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Lịch trực tiếp</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.SlideShow">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Banner - Hình ảnh</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "SlideShow", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách hình ảnh</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.Video">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Thư viện Video</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "GroupVideo", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách nhóm video</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Video", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách video</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.ImageLibrary">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Thư viện hình ảnh</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "GroupImageLibrary", new {area = "Admin"})">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách nhóm hình ảnh</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "ImageLibrary", new {area = "Admin"})">
                            <span class="icon-chevron-right"></span><span class="text">Danh sách hình ảnh</span>
                        </a>
                    </li>
                </ul>
            </li>
            <li class="openable @ViewBag.SiteSettings">
                <a href="#">
                    <span class="isw-list"></span><span class="text">Cấu hình</span>
                </a>
                <ul>
                    <li>
                        <a href="@Url.Action("Index", "SiteSetting", new { area = "Admin" })">
                            <span class="icon-chevron-right"></span><span class="text">Cấu hình</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "CacheManager", new {area = "Admin"})">
                            <span class="icon-chevron-right"></span><span class="text">Rebuil cache</span>
                        </a>
                    </li>
                </ul>
            </li>
        </ul>
        <div class="dr"><span></span></div>
    </div>
    <div class="content">
        @RenderBody()
    </div>
    <div class="ui-widget-overlay" style="width: 100%; height: 1141px; z-index: 1001; display: none"></div>

    <environment names="Development">
        s
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
